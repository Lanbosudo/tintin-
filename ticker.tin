#nop include multiplayer;

#tic hp_perf {
    hpbrief;

    #if {$inBattle == 1} {
        @getPowerUp{};

        #list perform cre @getPerform{};
        #foreach $perform[%*] zhaoshi {
            #repl zhaoshi {|} { };
            $zhaoshi;
        };


        #unvar zhaoshi perform;
    };
} {2}
